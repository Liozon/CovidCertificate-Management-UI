{
  "name": "cc-management-ui",
  "version": "4.8.3",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "release": "node scripts/release.js",
    "prerelease": "npm run lint && npm test -- --configuration production && npm run build -- --configuration production",
    "supportedBrowsers": "echo \"export const supportedBrowsers = $(browserslist-useragent-regexp --allowHigherVersions);\" > ./src/app/supportedBrowsers.ts",
    "lint": "ng lint",
    "format": "npm run lint -- --fix"
  },
  "private": true,
  "jestSonar": {
    "reportPath": "./coverage/sonarQube",
    "reportFile": "sqr.xml",
    "indent": 4,
    "sonar56x": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint"
    }
  },
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "@angular/forms": "^14.2.10",
    "@angular/localize": "^14.2.10",
    "@angular/material": "^13.3.9",
    "@angular/material-moment-adapter": "^13.3.9",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/router": "^14.2.10",
    "@ngx-translate/core": "^14.0.0",
    "@oblique/oblique": "^8.2.4",
    "@popperjs/core": "^2.9.2",
    "angular-auth-oidc-client": "^11.6.8",
    "moment": "^2.29.1",
    "rxjs": "~6.6.7",
    "time-input-polyfill": "^1.0.10",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^14.2.9",
    "@angular/compiler-cli": "^14.2.10",
    "@angular/language-service": "^14.2.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^13.13.51",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "browserslist-useragent-regexp": "^3.0.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^8.10.2",
    "typescript": "~4.6.4"
  }
}
